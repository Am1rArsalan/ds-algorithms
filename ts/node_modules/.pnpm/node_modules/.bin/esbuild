#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/esbuild@0.14.23/node_modules/esbuild/bin/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/esbuild@0.14.23/node_modules/esbuild/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/esbuild@0.14.23/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules:/home/amir/personal/advent-of-code/problems/node_modules:/home/amir/personal/advent-of-code/node_modules:/home/amir/personal/node_modules:/home/amir/node_modules:/home/node_modules:/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules/esbuild/bin/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules/esbuild/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/esbuild@0.14.23/node_modules/esbuild/bin/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/esbuild@0.14.23/node_modules/esbuild/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/esbuild@0.14.23/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules:/home/amir/personal/advent-of-code/problems/node_modules:/home/amir/personal/advent-of-code/node_modules:/home/amir/personal/node_modules:/home/amir/node_modules:/home/node_modules:/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules/esbuild/bin/node_modules:/home/amir/personal/advent-of-code/problems/ts/node_modules/.pnpm/node_modules/esbuild/node_modules"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
